<h1>タスク 作成</h1>
<%= form_with model: @task, local: true do |form|%>
  <div>
    <%= form.label :title,"タイトル"%>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :content, "内容" %>
    <%= form.text_field :content %>
  </div>
  <div>
    <%= form.submit "送信" %>
  </div>
<% end %>
<%= link_to "一覧画面へ", "/tasks" ,class:"list"%>